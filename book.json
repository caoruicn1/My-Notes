        },
        "anchor-navigation-ex": {
            "showLevel":false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "edit-link": {
            "base": "https://github.com/Chandlercjy/My-Notes/tree/master",
            "label": "Edit This Page"
        },
        "page-footer": {
            "description": "Modified at",
            "signature": "Chandler Chan",
            "wisdom": "E-mail: chenjiayicjy@gmail.com",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; Chandler Chan",
            "timeColor": "#BF360C",
            "copyrightColor": "#cc000",
            "isShowQRCode":false,
            "baseUri": "https://chandlercjy.gitbooks.io/chandler-s-notes/content/",
            "isShowIssues":false,
            "repo": "Chandlercjy/My-Notes",
            "issueNum": "8",
            "token": "6f104ca5f3cbd19eee48fa8c1a207fadad5a453a",
